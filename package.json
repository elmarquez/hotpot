{
  "name": "hotpot",
  "version": "0.1.0",
  "description": "Product discussion and co-creation",
  "scripts": {
    "build": "parcel build src/index.js --out-dir build --out-file bundle.js --public-url client",
    "build:dev": "NODE_ENV=development parcel build src/index.js --out-dir build --out-file bundle.js --public-url client",
    "lint": "eslint ./src/ || true",
    "precommit": "npm run lint",
    "serve": "node index.js",
    "test": "npm run lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "watch": "NODE_ENV=development parcel watch src/index.js --out-dir build --out-file bundle.js --public-url client",
    "webpack-build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --progress",
    "webpack-dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --inline --hot --progress",
    "webpack-prestart": "npm run build",
    "webpack-start": "serve . -s -c 1"
  },
  "private": true,
  "author": "Davis Marques <dmarques@unimelb.edu.au>",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "colors": "^1.3.0",
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.4",
    "prettier": "^1.13.7",
    "uglify-js": "^3.4.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-pino-logger": "^3.0.2",
    "feather-icons": "^4.7.3",
    "html2canvas": "^1.0.0-alpha.12",
    "moment": "^2.22.2",
    "mongodb": "~3.0.8",
    "mongoose": "^5.2.2",
    "pino": "^4.17.3",
    "promise-polyfill": "^6.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-animate-on-change": "^2.1.1",
    "react-dom": "^16.4.1",
    "react-feather": "^1.1.1",
    "react-habitat": "^1.0.1",
    "react-icon-base": "^2.1.2",
    "react-icons-kit": "^1.1.5",
    "react-pose": "^2.1.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:elmarquez/hotpot.git"
  },
  "bugs": {
    "url": "https://git@github.com:elmarquez/hotpot.git/issues"
  },
  "homepage": "https://git@github.com:elmarquez/hotpot.git#readme"
}
